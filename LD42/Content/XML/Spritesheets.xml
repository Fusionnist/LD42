<?xml version="1.0" encoding="utf-8" ?>
<Spritesheets>
  <Spritesheet name="placeholderSheet" source="Placeholder/placeholder1">
    //ITEM FRAME
    <Texture name="itemframe" id="itemframe">
     <Frame displayTime="0.1">
       <SourceRect x="0" y="0" w="32" h="32"></SourceRect>
       <Center x="0" y="0"></Center>
     </Frame>
     <Hitboxes>
       <Collection type="collision">
         <Frame>
            <Hitbox x="0" y="0" w="32" h="32"></Hitbox>
          </Frame>
        </Collection>
     </Hitboxes>
    </Texture>
   //ITEM
   <Texture name="item" id="item">
     <Frame displayTime="0.1">
        <SourceRect x="32" y="0" w="32" h="32"></SourceRect>
       <Center x="0" y="0"></Center>
      </Frame>
    </Texture>
    <Texture name="heart" id="item">
      <Frame displayTime="0.1">
        <SourceRect x="32" y="32" w="32" h="32"></SourceRect>
        <Center x="0" y="0"></Center>
      </Frame>
    </Texture>
   //PICKUP
   <Texture name="pickup" id="pickup">
      <Frame displayTime="0.1">
       <SourceRect x="0" y="32" w="16" h="16"></SourceRect>
       <Center x="0" y="0"></Center>
     </Frame>
     <Hitboxes>
       <Collection type="collision">
         <Frame>
           <Hitbox x="0" y="0" w="16" h="16"></Hitbox>
         </Frame>
       </Collection>
     </Hitboxes>
    </Texture>
    <Texture name="heart" id="pickup">
      <Frame displayTime="0.1">
        <SourceRect x="16" y="32" w="16" h="16"></SourceRect>
        <Center x="0" y="0"></Center>
      </Frame>
      <Hitboxes>
        <Collection type="collision">
          <Frame>
            <Hitbox x="0" y="0" w="16" h="16"></Hitbox>
          </Frame>
        </Collection>
      </Hitboxes>
    </Texture>
  </Spritesheet>
  <Spritesheet name="placeholderSheet2" source="Placeholder/barint">
    //spikes
    <Texture name="spikes" id="spikes">
      <Frame displayTime="0.1">
        <SourceRect x="0" y="0" w="20" h="3"></SourceRect>
        <Center x="0" y="0"></Center>
      </Frame>
      <Hitboxes>
        <Collection type="collision">
          <Frame>
            <Hitbox x="0" y="0" w="20" h="3"></Hitbox>
          </Frame>
        </Collection>
      </Hitboxes>
    </Texture>
  </Spritesheet>
  <Spritesheet name="pruns" source="Sprite/runningspritesheet">
    //spikes
    <Texture name="run" id="player">
      <Frameset delay="0.1" x="32" y="32" w="32" h="32" amt="8" cx="0" cy="0"></Frameset>
      <Hitboxes>
        <Collection type="collision">
          <Frameset x="8" y="0" w="16" h="32" amt="8"></Frameset>
        </Collection>
      </Hitboxes>
    </Texture>
  </Spritesheet>
  <Spritesheet name="pjumps" source="Sprite/jumpspritesheet">
    //spikes
    <Texture name="jump" id="player">
      <Frameset delay="0.1" x="32" y="32" w="32" h="32" amt="4" cx="0" cy="0"></Frameset>
      <Hitboxes>
        <Collection type="collision">
          <Frameset x="8" y="0" w="16" h="32" amt="4"></Frameset>
        </Collection>
      </Hitboxes>
    </Texture>
  </Spritesheet>
  //walk enemy
  <Spritesheet name="walkenemys" source="Sprite/enemyspritesheet">
  <Texture name="walk" id="enemy">
    <Frameset delay="0.1" x="32" y="32" w="32" h="32" amt="8" cx="0" cy="0"></Frameset>
    <Hitboxes>
      <Collection type="collision">
        <Frameset x="8" y="0" w="16" h="32" amt="8"></Frameset>
      </Collection>
    </Hitboxes>
  </Texture>
</Spritesheet>
  //fish
  <Spritesheet name="fishidles" source="Sprite/fishidle">
    <Texture name="idle" id="fish">
      <Frameset delay="0.1" x="32" y="32" w="32" h="32" amt="3" cx="0" cy="0"></Frameset>
      <Hitboxes>
        <Collection type="collision">
          <Frameset x="8" y="0" w="16" h="32" amt="3"></Frameset>
        </Collection>
      </Hitboxes>
    </Texture>
  </Spritesheet>
  <Spritesheet name="fishappears" source="Sprite/fishwakeup">
    <Texture name="appear" id="fish" loop="false">
      <Frameset delay="0.1" x="32" y="32" w="32" h="32" amt="5" cx="0" cy="0"></Frameset>
      <Hitboxes>
        <Collection type="collision">
          <Frameset x="8" y="0" w="16" h="32" amt="5"></Frameset>
        </Collection>
      </Hitboxes>
    </Texture>
  </Spritesheet>
  //shadow
  <Spritesheet name="shadows" source="Sprite/shadow">
    <Texture name="shadow" id="shadow">
      <Frameset delay="0.1" x="0" y="0" w="32" h="32" amt="1" cx="0" cy="0"></Frameset>
      <Hitboxes>
        <Collection type="collision">
          <Frameset x="8" y="0" w="16" h="32" amt="1"></Frameset>
        </Collection>
      </Hitboxes>
    </Texture>
  </Spritesheet>
  <Spritesheet name="slotappears" source="UI/invenappear">
    <Texture name="appear" id="slot" loop="false">
      <Frameset delay="0.05" x="0" y="0" w="64" h="64" amt="5" cx="0" cy="0"></Frameset>     
    </Texture>
  </Spritesheet>
</Spritesheets>